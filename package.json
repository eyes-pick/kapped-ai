    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf ./node_modules ./package-lock.json ./.next",
    "update": "npm update",
    "clear:prompt": "npx tsx scripts/clear-prompt.ts",
    "validate:templates": "zod-cli validate lib/templates.json",
    "sandbox": "concurrently \"next dev -p 3001\" \"npm --prefix ./sandbox-vite run dev\"",
    "typecheck": "tsc --noEmit",
    "setup:build": "npm run lint && npm run typecheck && npm run test && npm run build",
    "setup:dev": "npm run lint && npm run typecheck && npm run test && npm run dev",
    "setup:nolint": "npm run lint || true && npm run typecheck && vitest run && npm run dev",
    "setup:full": "npm run format:check && npm run lint && npm run typecheck && vitest run && npm run build",
    "setup:quick": "vitest run && npm run build",
    "setup:ci": "npm run format:check && npm run lint && npm run typecheck && vitest run && npm run build",
    "setup:dev:nolint": "npm run typecheck && vitest run && npm run dev",
    "check": "npm audit && npm outdated || true"
